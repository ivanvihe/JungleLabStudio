                   info['note_on_mapping'] = {
                       'action_id': action_id,
                       'mapping_data': mapping_data
                   }
                   info['has_mapping'] = True
                   
               if note_off_key in self.midi_lookup:
                   action_id, mapping_data = self.midi_lookup[note_off_key]
                   info['note_off_mapping'] = {
